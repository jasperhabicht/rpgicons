\cs_new:Npn \rpgicons_roll_syntax_convert:n #1 {
    \str_case:nnF {#1} {
        { 2 } { twoside }
        { 4 } { fourside }
        { 6 } { sixside }
        { 8 } { eightside }
        { 10 } { tenside }
        { 12 } { twelveside }
        { 20 } { twentyside }
        { 100 } { hundredside }
